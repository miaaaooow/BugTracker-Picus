<h1>Create a New Project</h1>

<% form_for(@project) do |f| %>
  <p>
    <%= f.label :name  %>
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>
  <% f.error_messages %>
  <p>
    <%= f.submit 'Create in Picus' %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>
